
<Router AppAssembly="@typeof(Program).Assembly" AdditionalAssemblies="new[] { typeof(RazorClassLibrary31.Pages.EmployeePage.EmployeeList).Assembly}">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(RazorClassLibrary31.Shared.MainLayout)" />
    </Found>
