<div id="employeeEditModal" class="modal fade @Show" tabindex="-1" role="dialog" aria-labelledby="employeeEditModalLabel" aria-hidden="@AriaHidden" style="display:@Display; padding-right: @PaddingRight">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="employeeEditModalLabel">Edit Employee</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" @onclick="() => CloseEditForm()">
                    <span aria-hidden="@AriaHidden">×</span>
                </button>
            </div>
            <div class="modal-body">
                <p>Woohoo, you're reading this text in a modal!</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal" @onclick="() => CloseEditForm()">Close</button>
                <button type="button" class="btn btn-primary">Save</button>
            </div>
        </div>
    </div>
</div>


